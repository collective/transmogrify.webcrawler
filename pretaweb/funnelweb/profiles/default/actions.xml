<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <object name="object_buttons" meta_type="CMF Action Category">
    <object name="folderfunnelweb" meta_type="CMF Action" i18n:domain="plone">
      <property name="title" i18n:translate="">Import from website...</property>
      <property name="description" i18n:translate="">Import a website into the current folder</property>
      <property name="url_expr">string:${globals_view/getCurrentFolderUrl}/@@funnelwebimport</property>
      <property
          name="available_expr"></property>
      <property name="permissions">
        <element value="Add portal content"/>
      </property>
      <property name="visible">True</property>
    </object>
  </object>
</object>